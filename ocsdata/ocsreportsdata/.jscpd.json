{
  "threshold": 10,
  "reporters": ["html", "markdown"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.rbenv/**",
    "**/.venv'/**",
    "**/*cache*/**",
    "**/.github/**",
    "**/.idea/**",
    "**/report/**",
    "**/*.svg",
    "**/config",
    "**/libraries",
    "**/js",
    "**/files",
    "**/css",
    "**/templates"
  ],
  "min-tokens": 150,
  "min-lines": 40
}
